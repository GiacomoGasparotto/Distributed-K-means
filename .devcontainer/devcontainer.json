{
  "name": "Spark Jupyter Workspace",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "jupyter-notebook",
  "workspaceFolder": "/mapd-workspace",
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "jupyter.notebookFileRoot": "/mapd-workspace"
  },
  "postCreateCommand": "pip install --upgrade pip"
}